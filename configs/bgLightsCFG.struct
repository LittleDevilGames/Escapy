//Escapy bg lights cfg
#type struct

lights {

	containers[0]: {
		type {
			glProgram.glBlendFuncSeparate: (GL_SRC_ALPHA, GL_ONE, GL_ONE, GL_ONE_MINUS_SRC_COLOR);
			shader.builtIn: SOFT_COLOR_DODGE
			blur: false;
		}
		stepShift(0, 0);
		source[0]: {
			EscapyStdLight : {
				srcType: RND_64
				position: (50, 200)
				color (250, 250, 250)
				scale: 7.5
				visible true
				shift.orto.ext.file = "orto.struct";
		}
	}
}
